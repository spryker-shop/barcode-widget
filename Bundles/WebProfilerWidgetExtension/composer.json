{
  "name": "spryker-shop/web-profiler-extension",
  "description": "WebProfilerWidgetExtension module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/container": "*",
    "spryker/symfony": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "SprykerShop\\": "src/SprykerShop/"
    }
  },
  "suggest": {
    "spryker/container": "If you want to use the Container module.",
    "spryker/symfony": "If you want to use the Symfony component."
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
