{
  "name": "spryker-shop/product-quick-add-widget",
  "type": "library",
  "description": "ProductQuickAddWidget module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/application": "^3.10.0",
    "spryker/kernel": "^3.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker-shop/product-search-widget": "^1.0.0",
    "spryker/util-encoding": "^2.0.0"
  },
  "require-dev": {
    "spryker-shop/shop-application": "*",
    "spryker/code-sniffer": "*",
    "spryker/silex": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "SprykerShop\\": "src/SprykerShop/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "suggest": {
    "spryker/silex": "Use this module when using plugins that need Silex dependencies."
  }
}
