{
  "name": "spryker-shop/cms-content-widget-banner",
  "type": "library",
  "description": "CmsContentWidgetBanner module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/cms-content-widget": "^1.0.0",
    "spryker/kernel": "^3.24.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*",
    "spryker/twig": "*"
  },
  "suggest": {
    "spryker/twig": "Use this module when using plugins that need Twig dependencies."
  },
  "autoload": {
    "psr-4": {
      "SprykerShop\\": "src/SprykerShop/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
