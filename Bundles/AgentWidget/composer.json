{
  "name": "spryker-shop/agent-widget",
  "description": "AgentWidget module",
  "license": "proprietary",
  "require": {
    "spryker/kernel": "^3.23.0",
    "spryker/agent": "^1.0.0",
    "spryker-shop/shop-ui": "^1.2.0",
    "symfony/http-foundation": "^2.3.0 || ^3.0.0",
    "symfony/validator": "^2.6.13 || ^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "SprykerShop\\": "src/SprykerShop/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerShopTest\\": "tests/SprykerShopTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
