<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="QuickOrder">
        <property name="items" type="QuickOrderItem[]" />
    </transfer>

    <transfer name="QuickOrderItem">
        <property name="sku" type="string" />
        <property name="qty" type="int" />
    </transfer>

    <transfer name="QuickOrderProductAdditionalData">
        <property name="idProductConcrete" type="int" />
    </transfer>

    <transfer name="QuickOrderProductAdditionalData">
        <property name="idProductConcrete" type="int" />
    </transfer>

    <transfer name="QuickOrderProductPrice">
        <property name="idProductConcrete" type="int" />
        <property name="total" type="string" />
        <property name="quantity" type="int" />
        <property name="currentProductPrice" type="CurrentProductPrice" />
        <property name="currency" type="Currency" />
    </transfer>

</transfers>
