{% extends model('template') %}

{% define data = {
    quantity: _view.quantity,
    price: _view.price,
} %}

{% block template %}
    {% embed molecule('quick-order-row-partial', 'QuickOrderPage') with {
        data: {
            quantity: data.quantity,
            price: data.price
        }
    } only %}
        {% block component %}
            {{block('body')}}
        {% endblock %}
    {% endembed %}
{% endblock %}
