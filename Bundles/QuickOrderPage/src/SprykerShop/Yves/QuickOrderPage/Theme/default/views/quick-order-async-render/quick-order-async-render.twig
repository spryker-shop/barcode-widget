{% extends model('template') %}

{% define data = {
    form: _view.form,
    additionalDataColumnProviderPlugins: _view.additionalDataColumnProviderPlugins,
    productConcretesData: _view.productConcretesData
} %}

{% block template %}
    {% include molecule('quick-order-form-field-list', 'QuickOrderPage') with {
        data: {
            items: data.form.items,
            additionalDataColumnProviderPlugins: data.additionalDataColumnProviderPlugins,
            productConcretesData: data.productConcretesData
        }
    } only %}
{% endblock %}
