{
  "name": "spryker-shop/quick-order-page",
  "description": "QuickOrderPage module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker-shop/cart-page": "^0.4.0 || ^0.5.0 || ^0.6.0",
    "spryker-shop/checkout-page": "^0.3.0",
    "spryker-shop/shop-application": "^0.3.0",
    "spryker/application": "^3.0.0",
    "spryker/cart": "^4.0.0 || ^5.0.0",
    "spryker/kernel": "^3.19.0",
    "spryker/messenger": "^3.1.0",
    "spryker/symfony": "^3.1.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/silex": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/silex": "If you want to use ServiceProvider."
  },
  "autoload": {
    "psr-4": {
      "SprykerShop\\": "src/SprykerShop/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
