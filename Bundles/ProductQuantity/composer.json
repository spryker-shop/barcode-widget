{
  "name": "spryker-shop/product-quantity",
  "type": "library",
  "description": "ProductQuantity module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker-shop/shopping-list-page-extension": "^1.1.0",
    "spryker/kernel": "^3.16.0",
    "spryker/product-quantity-storage": "^1.5.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/url-storage": "Use this module when using plugins that need UrlStorage dependencies."
  },
  "autoload": {
    "psr-4": {
      "SprykerShop\\": "src/SprykerShop/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
