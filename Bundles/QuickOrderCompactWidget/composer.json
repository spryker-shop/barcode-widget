{
  "name": "spryker-shop/quick-order-compact-widget",
  "description": "QuickOrderCompactWidget module",
  "license": "proprietary",
  "require": {
    "php": "7.1",
    "spryker/kernel": "^3.0.0",
    "spryker/product-page-search": "^2.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker-shop/quick-order-page": "*",
    "spryker-shop/product-search-widget": "*",
    "spryker-shop/shop-application": "*",
  },
  "require-dev": {
