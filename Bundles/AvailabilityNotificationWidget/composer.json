{
  "name": "spryker-shop/availability-notification-widget",
  "type": "library",
  "description": "AvailabilityNotificationWidget module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/application": "^3.8.0",
    "spryker/kernel": "^3.24.0",
    "spryker/availability-notification": "^4.3.0",
    "spryker/symfony": "^3.1.0"
  },
  "require-dev": {
    "spryker-shop/product-detail-page": "*",
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker-shop/product-detail-page": "if you want to use AvailabilityNotificationWidget"
  },
  "autoload": {
    "psr-4": {
      "SprykerShop\\": "src/SprykerShop/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
