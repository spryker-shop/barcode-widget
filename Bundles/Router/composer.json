{
  "name": "spryker-shop/router",
  "type": "library",
  "description": "Router module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker-shop/router-extension": "^1.0.0",
    "spryker/application-extension": "^1.0.0",
    "spryker/container": "^1.1.0",
    "spryker/event-dispatcher-extension": "^1.0.0",
    "spryker/kernel": "^3.30.0",
    "spryker/log": "^3.0.0",
    "spryker/symfony": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/event-dispatcher": "*",
    "spryker/silex": "*",
    "spryker/testify": "*",
    "spryker/util-text": "*",
    "spryker/zend": "*"
  },
  "suggest": {
    "spryker/event-dispatcher": "If you want to use the RouterLocaleEventDispatcher plugin.",
    "spryker/silex": "If you want to use the RouterListenerEventDispatcher plugin.",
    "spryker/util-text": "If you want to use the RouterEnhancer plugins.",
    "spryker/zend": "If you want to use the Router plugin."
  },
  "autoload": {
    "psr-4": {
      "SprykerShop\\": "src/SprykerShop/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerShopTest\\": "tests/SprykerShopTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
